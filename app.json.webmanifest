{
  "name": "Passes",
  "short_name": "Passes",
  "start_url": "./",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#c1445b",
  "prefer_related_applications": false,
  "icons": [
    {
      "src": "icon.svg",
      "type": "image/svg+xml",
      "sizes": "any"
    },
    {
      "src": "monochrome.icon.svg",
      "type": "image/svg+xml",
      "sizes": "any",
      "purposes": "monochrome"
    },
    {
      "src": "maskable.icon.svg",
      "type": "image/svg+xml",
      "sizes": "any",
      "purposes": "maskable"
    },
    {
      "src": "splash.icon.svg",
      "type": "image/png",
      "sizes": "2001x2001"
    }
  ],
  "file_handlers": [
    {
      "action": "/open",
      "accept": {
        "application/vnd.apple.pkpass": [
          ".pkpass"
        ],
        "application/vnd.apple.pkpasses": [
          ".pkpasses"
        ]
      },
      "icons": [
        {
          "src": "icon.svg",
          "type": "image/svg+xml",
          "sizes": "any"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "share_target": {
    "action": "/open",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title" : "title",
      "files": [
        {
          "name": "digital pass",
          "accept": [
            ".pkpass",
            ".pkpasses",
            "application/zip",
            "application/octet-stream",
            "application/vnd.apple.pkpass",
            "application/vnd.apple.pkpasses",
            "application/vnd.apple.pkpass+zip",
            "application/vnd.apple.pkpasses+zip",
            "application/vnd.apple.pkpass+octet-stream",
            "application/vnd.apple.pkpasses+octet-stream"
          ]
        }
      ]
    }
  }
}